CHIP DecoderSegment201839113 {
    IN A, B, C, D;
    OUT c;

    PARTS:
    // Inverters
    Not(in=B, out=nB);
    Not(in=C, out=nC);
    Not(in=D, out=nD);

    And(in=nB, in2=nC, out=tmp1);
    And(in=tmp1, in2=nD, out=term1);
    
    And(in=A, in2=B, out=term2);

    And(in=A, in2=C, out=term3);

    And(in=C, in2=nD, out=term4);
    
    Or(in=term1, in2=term2, out=or1);
    Or(in=term3, in2=term4, out=or2);
    Or(in=or1, in2=or2, out=c);
}
